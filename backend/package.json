{
  "name": "devops-monitoring-platform",
  "version": "1.0.0",
  "description": "DevOps Monitoring Platform Backend Services",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:user\" \"npm run dev:monitoring\" \"npm run dev:ai\" \"npm run dev:notification\"",
    "dev:user": "cd services/user && npm run dev",
    "dev:monitoring": "cd services/monitoring && npm run dev",
    "dev:ai": "cd services/ai && npm run dev",
    "dev:notification": "cd services/notification && npm run dev",
    "build": "npm run build:user && npm run build:monitoring && npm run build:ai && npm run build:notification",
    "build:user": "cd services/user && npm run build",
    "build:monitoring": "cd services/monitoring && npm run build",
    "build:ai": "cd services/ai && npm run build",
    "build:notification": "cd services/notification && npm run build",
    "db:generate": "cd services/user && npx prisma generate && cd ../monitoring && npx prisma generate && cd ../ai && npx prisma generate && cd ../notification && npx prisma generate",
    "db:push": "cd services/user && npx prisma db push && cd ../monitoring && npx prisma db push && cd ../ai && npx prisma db push && cd ../notification && npx prisma db push",
    "db:migrate": "cd services/user && npx prisma migrate dev && cd ../monitoring && npx prisma migrate dev && cd ../ai && npx prisma migrate dev && cd ../notification && npx prisma migrate dev"
  },
  "keywords": [
    "devops",
    "monitoring",
    "microservices",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.16.0",
    "@sendgrid/mail": "^8.1.5",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "events": "^3.3.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.543.0",
    "morgan": "^1.10.1",
    "next": "^15.5.3",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "openai": "^5.20.1",
    "rate-limit-redis": "^4.2.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.27.8",
